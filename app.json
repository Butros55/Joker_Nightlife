      },
      "package": "com.project.Joker_Nightlife"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8eef4d42-7138-4ceb-af5b-54f1b0804a76"
      }
    },
    "owner": "butros55",
    "plugins": [
      "expo-apple-authentication"
    ]
